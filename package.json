{
  "name": "@appr/root",
  "private": true,
  "description": "Monorepo root for @appr",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "clean": "rm -rf yarn.lock node_modules packages/*/node_modules apps/*/node_modules packages/*/dist apps/*/dist && yarn cache clean --force",
    "build": "yarn workspaces foreach -pi run build",
    "start": "yarn workspaces foreach -pi run start",
    "test": "yarn workspaces foreach -pi run test"
  },
  "keywords": [],
  "author": "Evan Bremer",
  "license": "MIT"
}
